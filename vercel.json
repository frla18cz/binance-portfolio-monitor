{
  "functions": {
    "api/index.py": {},
    "api/dashboard.py": {}
  },
  "crons": [
    {
      "path": "/api/index",
      "schedule": "0 * * * *"
    }
  ],
  "routes": [
    {
      "src": "/dashboard",
      "dest": "/api/dashboard"
    },
    {
      "src": "/api/dashboard/(.*)",
      "dest": "/api/dashboard"
    }
  ]
}