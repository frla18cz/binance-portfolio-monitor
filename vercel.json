{
  "functions": {
    "api/*.py": {
      "regions": ["fra1"]
    }
  },
  "crons": [
    {
      "path": "/api/monitor",
      "schedule": "0 * * * *"
    }
  ],
  "routes": [
    {
      "src": "/dashboard",
      "dest": "/api/dashboard"
    },
    {
      "src": "/api/dashboard/(.*)",
      "dest": "/api/dashboard"
    }
  ]
}