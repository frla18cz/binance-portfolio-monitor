{
  "functions": {
    "api/*.py": {
      "regions": ["fra1"],
      "runtime": "python3.9"
    }
  },
  "buildCommand": "pip install -r requirements.txt",
  "crons": [
    {
      "path": "/api/index",
      "schedule": "*/5 * * * *"
    }
  ],
  "routes": [
    {
      "src": "/dashboard",
      "dest": "/api/dashboard"
    },
    {
      "src": "/api/dashboard/(.*)",
      "dest": "/api/dashboard"
    }
  ]
}